{
  "name": "tplus-Marketing",
  "version": "1.0.1",
  "description": "touch protal",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env PUBLIC_TYPE=offline DEV_ENV=mobile PROXY_PORT=4000 node node_modules/tplus-devtools/bin/start",
    "build": "rimraf dist && cross-env DEV_ENV=mobile CIA_ENV=inte node node_modules/tplus-devtools/bin/entry",
    "dev": "rimraf dist && cross-env NODE_ENV=offline node node_modules/tplus-devtools/bin/dev ../pos",
    "pub": "npm run build && npm publish --registry https://registry.npmjs.org",
    "test": "set DEBUG=tplus:*,-not_this && mocha",
    "install": "node node_modules/tplus-devtools/bin/install",
    "zip": "npm run install && npm run build && node bin/zip"
  },
  "keywords": [
    "tplus",
    "queryScheme"
  ],
  "author": "tplus",
  "license": "ISC",
  "dependencies": {
    "antd-mobile": "^2.2.13",
    "base-64": "^0.1.0",
    "core-decorators": "^0.12.0",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "moment": "^2.22.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dom": "^16.8.6",
    "tplus-api": "^1.0.175",
    "tplus-mobile-components": "^1.0.5",
    "tplus-mobile-navigator": "^1.0.13",
    "tplus-mobile-plugin-gzq": "^1.0.15",
    "tplus-mobilelogin": "^1.0.23",
    "tplus-query-scheme": "^1.0.7",
    "tplusfx": "^3.0.196",
    "vconsole": "^3.3.4",
    "zevent": "^1.1.1"
  },
  "devDependencies": {
    "archiver": "^1.1.0",
    "babel-eslint": "^8.0.3",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "quick-local-ip": "^1.0.7",
    "react-motion": "^0.5.2",
    "tplus-devtools": "^3.0.76"
  }
